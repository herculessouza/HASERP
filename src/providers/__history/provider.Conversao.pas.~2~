unit provider.Conversao;

interface


uses
SysUtils,
StrUtils,
Classes;


type
    TPCPessoas = (tpCliente, tpFornecedores, tpFuncionarios);

      function  PessoasToInt(const t: TPCPessoas): integer;

implementation
  //implementando pessoas

      function  PessoasToInt(const t: TPCPessoas): integer;
      begin
        case t of
          tpCliente:              Result := 1 ;
          tpFornecedores:         Result := 2 ;
          tpFuncionarios:         Result := 3 ;
        end;
      end;

end.
